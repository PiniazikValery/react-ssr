{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Test ssr application",
  "scripts": {
    "dev": "npm run build-server && npm-run-all --parallel dev:*",
    "build-server": "webpack --config webpack.server.js",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PiniazikValery/react-ssr.git"
  },
  "keywords": [
    "ssr",
    "react"
  ],
  "author": "Valery Piniazik",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PiniazikValery/react-ssr/issues"
  },
  "homepage": "https://github.com/PiniazikValery/react-ssr#readme",
  "dependencies": {
    "express": "^4.17.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  }
}
